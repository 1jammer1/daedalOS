{
  "plugins": [
    "postcss-flexbugs-fixes",
    "postcss-safe-parser",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009",
          "grid": "autoplace"
        },
        "stage": 3,
        "features": {
          "custom-properties": false
        },
        "parser": "safe"
      }
    ]
  ]
}
